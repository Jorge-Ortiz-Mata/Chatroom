<%- if controller_name != 'sessions' %>
  <%= link_to "Log in", new_session_path(resource_name), class:'text-white underline font-semibold text-sm mt-5' %>
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Sign up", new_registration_path(resource_name), class:'text-white underline font-semibold text-sm mt-5' %>
<% end %>
